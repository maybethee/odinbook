<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>

<div>
  <%= link_to "new post", new_post_path, data: { turbolinks: false } %> 
</div>
<br>

<% @posts.each do |post| %>
  <div>
    <%= link_to "@#{post.user.username}", user_profile_path(post.user)%><br>
    <%= post.content %><br>
    <%= link_to post.updated_at.strftime("%m/%d/%Y %I:%M%p"), post_path(post) %>
  </div><br>
<% end %>
